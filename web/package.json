  "version": "1.0.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build-assets && yarn build-pages",
    "dev": "concurrently 'yarn build-assets-watch' 'yarn build-pages-dev'",
    "build-assets": "rollup -c rollup.config.js",
    "build-assets-watch": "rollup -c rollup.config.js -w",
    "build-pages": "eleventy",
    "build-pages-dev": "eleventy --watch --serve",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "clean": "rm -Rf ./_site/*",
    "kill-11ty": "kill $(ps -e | grep .bin/eleventy | awk '{print $1}')"
  },
