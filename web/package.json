{
  "name": "sanity-eleventy-blog-web",
  "version": "1.0.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn js-build && eleventy",
    "dev": "concurrently 'yarn js-build-watch' 'eleventy --watch --serve'",
    "now-build": "yarn build",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "clean": "rm -Rf ./_site/posts/*",
    "kill-11ty": "kill $(ps -e | grep .bin/eleventy | awk '{print $1}')"
  },
